<!-- Form venue start -->
<%= form_with(model: @venue, local: true) do |f| %>
<!-- Form venue end -->

  <!-- Label and input for state start -->
  <div class="form-group">
    <%= f.label :state %>

    <!-- Input for state start -->
    <%= f.text_field :state, class: "form-control" %>
    <!-- Input for state end -->
  </div>
  <!-- Label and input for state end -->

  <!-- Label and input for city start -->
  <div class="form-group">
    <%= f.label :city %>

    <!-- Input for city start -->
    <%= f.text_field :city, class: "form-control" %>
    <!-- Input for city end -->
  </div>
  <!-- Label and input for city end -->

  <!-- Label and input for neighborhood start -->
  <div class="form-group">
    <%= f.label :neighborhood %>

    <!-- Input for neighborhood start -->
    <%= f.text_field :neighborhood, class: "form-control" %>
    <!-- Input for neighborhood end -->
  </div>
  <!-- Label and input for neighborhood end -->

  <!-- Label and input for cuisine_id start -->
  <div class="form-group">
    <%= f.label :cuisine_id %>

    <!-- Input for cuisine_id start -->
    <%= f.text_field :cuisine_id, class: "form-control" %>
    <!-- Input for cuisine_id end -->
  </div>
  <!-- Label and input for cuisine_id end -->

  <!-- Label and input for user_id start -->
  <!-- Input for user_id start -->
  <%= f.hidden_field :user_id, value: @user.id %>
  <!-- Input for user_id end -->
  <!-- Label and input for user_id end -->

  <%= f.submit "Create venue", class: "btn btn-block btn-outline-secondary" %>
<% end %>
